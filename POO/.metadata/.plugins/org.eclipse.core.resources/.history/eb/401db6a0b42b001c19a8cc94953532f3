package produtos;

public abstract class ProdutoInternacional {
	protected String nome;
	protected double preço;
	
	public Produtos(String nome, double preço) {
		this.nome @Override
	= nome;
		this.preço = preço;
	}
	public abstract double pegarPreço;
	public String pegarNome() {
		return this.nome;		
	}
	public double pegarPreço() {
		// TODO Auto-generated method stub
		return 0;
	}

}
